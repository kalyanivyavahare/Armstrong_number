package package12;

public class Reversnum {
	public int rveresNumber(int a) {
		int revers=0;
		while(a>0) {
			int rem=a%10;
			revers=revers*10+rem;
			a=a/10;
			
	}
		return revers;
	}
	public static void main(String[] args) {
		Reversnum ref=new Reversnum();
		int start=100;
		int end=300;
		for(int a=start;a<=end;a++) {
			System.out.println(ref.rveresNumber(a) );
		}
	}

}
